{
  "name": "check-in",
  "version": "0.0.0",
  "description": "",
  "keywords": [],
  "homepage": "https://github.com/xg4/check-in#readme",
  "bugs": {
    "url": "https://github.com/xg4/check-in/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xg4/check-in.git"
  },
  "license": "MIT",
  "author": "xg4 <xingor4@gmail.com>",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --watch src --ext ts --exec npm run serve",
    "lint": "eslint src --fix",
    "fmt": "prettier --write src",
    "serve": "ts-node --files --transpile-only ./src/index.ts",
    "start": "pm2 start ts-node -- --files --transpile-only ./src/index.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{json,md,yml}": [
      "prettier --write",
      "git add"
    ],
    "*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "async-retry": "^1.3.1",
    "cookie": "^0.4.1",
    "dayjs": "^1.10.4",
    "dotenv": "^8.2.0",
    "puppeteer": "^7.1.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/async-retry": "^1.4.2",
    "@types/cookie": "^0.4.0",
    "@types/node": "^14.14.31",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^5.0.9",
    "lint-staged": "^10.5.4",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1"
  }
}
